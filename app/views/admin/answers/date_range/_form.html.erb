<%= semantic_form_for [:admin, @answer], builder: ActiveAdmin::FormBuilder do |f|%>
<%= f.inputs "Question Type" do%>
<%= f.input :question, :input_html => { :readonly => true } %>
<%= f.object.question.body%>
<%= f.inputs name: 'Additional information', for: :config do |g|%>
<%= g.input :type_answer, as: :boolean%>
<%= f.object.question.config[:start]%>
<%= f.object.question.config[:finish]%>
<%= g.input :start, :as => :string, :input_html => {:class => 'datepicker hasDatePicker'}%>
<%= g.input :finish,:as => :string, :input_html => {:class => 'datepicker hasDatePicker'}%>

<%end%>
<%= f.input :body %>
<%end%>
<%= f.actions %>
<%end%>
